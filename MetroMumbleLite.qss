* {
  border: 1px solid transparent;
  font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
  font-size: 14px;
  margin: 0;
  padding: 0;
  selection-background-color: #298ce1;
  selection-color: white;
}

DockTitleBar {
  font-size: 11px;
}

QMainWindow::separator {
  background-color: #eeeeee;
  height: 5px;
  width: 5px;
}

QMainWindow::separator:hover {
  background: #dddddd;
}

QTextEdit {
  background-color: #eeeeee;
  color: #444444;
}

a {
  color: #2a749e;
  text-decoration: none;
}

QToolBar {
  background-color: #eeeeee;
  spacing: 0;
}

QToolBar::separator {
  background: #cccccc;
  height: 1px;
  margin: 5px;
  width: 1px;
}

QToolBar::handle {
  padding: 3px;
}

QToolBar::handle:horizontal {
  image: url(skin:handle_horizontal.svg);
  width: 8px;
}

QToolBar::handle:vertical {
  height: 8px;
  image: url(skin:handle_vertical.svg);
}

QToolButton {
  padding: 5px;
}

QToolButton:hover {
  background-color: #dddddd;
}

QHeaderView {
  border: 0;
  border-bottom: 1px solid #cccccc;
}

QHeaderView::section {
  background-color: white;
  border: 0;
  color: #111111;
  padding: 5px;
}

QTreeView {
  alternate-background-color: white;
  background-color: white;
  border: 1px solid #cccccc;
  color: #111111;
  outline: 0;
  selection-background-color: white;
  selection-color: #111111;
}

QTreeView.UserView, QTextBrowser.LogTextBrowser {
  padding: 1px;
}

QTreeView::item {
  border-left: 1px solid transparent;
  padding: 5px;
}

QTreeView::item.UserView {
  padding: 0;
}

QTreeView::item:hover {
  background-color: whitesmoke;
}

QTreeView::item:selected {
  background-color: whitesmoke;
  border: 1px solid transparent;
}

QTreeView::item:selected.UserView {
  border: 1px solid #cccccc;
}

QTreeView::branch {
  border-image: none;
  image: none;
  margin-left: 3px;
  margin-top: 1px;
  padding-left: 3px;
}

QTreeView::branch:has-children:closed {
  image: url(skin:branch_closed.svg);
}

QTreeView::branch:has-children:open {
  image: url(skin:branch_open.svg);
}

QTextBrowser {
  background-color: white;
  border: 1px solid #cccccc;
  color: #111111;
  padding: 1px;
}

.log-time {
  background-color: white;
  color: #95a5a6;
  font-size: 12px;
}

.log-server {
  background-color: white;
  color: #F9655D;
  font-weight: bold;
}

.log-channel {
  background-color: white;
  color: #e67e22;
  font-weight: bold;
}

.log-privilege {
  background-color: white;
  color: #c0392b;
  font-weight: bold;
}

.log-target {
  background-color: white;
  color: #27ae60;
  font-weight: bold;
}

.log-source {
  background-color: white;
  color: #27ae60;
  font-weight: bold;
}

QScrollBar {
  background: white;
  padding-left: 5px;
  width: 16px;
}

QScrollBar::handle {
  background: #cccccc;
}

QScrollBar::handle:hover {
  background: #dddddd;
}

QScrollBar::left-arrow,
QScrollBar::right-arrow,
QScrollBar::up-arrow,
QScrollBar::down-arrow,
QScrollBar::sub-line,
QScrollBar::add-line,
QScrollBar::add-page,
QScrollBar::sub-page,
QAbstractScrollArea::corner {
  background: #eeeeee;
  height: 0;
}

QMenuBar {
  background-color: #eeeeee;
}

QMenuBar::item {
  background-color: transparent;
  color: #444444;
  padding: 5px 12px;
}

QMenuBar::item:selected {
  background: #298ce1;
  color: white;
}

QMenuBar::item:pressed {
  background: #1979ca;
  color: white;
}

QMenu {
  background: white;
  border: 1px solid #cccccc;
  color: #111111;
  padding: 0;
}

QMenu::item {
  background: transparent;
  border: 1px solid transparent;
  color: #111111;
  margin: 1px;
  padding: .4em 2em;
}

QMenu::item:selected {
  background: #e4f3f7;
  border: 1px solid #97b5c6;
}

QMenu::item:disabled {
  color: #bbbbbb;
}

QMenu::separator {
  background: #cccccc;
  height: 1px;
}

QMenu::indicator {
  height: 20px;
  width: 25px;
}

QPushButton {
  background-color: transparent;
  color: #111111;
  font-size: 18px;
  padding: 2px 8px;
}

QPushButton:hover {
  background-color: #298ce1;
  color: white;
}

QPushButton:pressed {
  background-color: #1979ca;
}

QPushButton:disabled {
  color: #bbbbbb;
}

QDialog {
  background-color: #eeeeee;
  color: #111111;
}

QGroupBox {
  background-color: #fafafa;
  border: 1px solid #cccccc;
  color: #111111;
  font-size: 18px;
  margin: 5px;
  padding: 5px;
  padding-top: 30px;
}

QGroupBox::title {
  background-color: transparent;
  margin: 5px;
  margin-left: 10px;
  margin-right: 10px;
}

QListView {
  background-color: white;
  border: 1px solid #cccccc;
  min-width: 160px;
  outline: 0;
}

QListView#qlwIcons {
  background-color: transparent;
  border: 0;
  font-size: 16px;
  margin-left: 5px;
  margin-top: 15px;
}

QListView::item {
  border-left: 10px solid transparent;
  color: #444444;
  padding: 5px 0;
}

QListView::item:hover {
  background-color: whitesmoke;
}

QListView::item:selected {
  background-color: #fafafa;
  border: 1px solid #cccccc;
  padding-left: 9px;
}

QTabWidget::pane {
  background-color: #fafafa;
  border: 1px solid #cccccc;
  margin-left: 5px;
}

QTabWidget::tab-bar {
  left: 5px;
}

QTabBar::tab {
  background-color: transparent;
  color: #111111;
  margin-bottom: -1px;
  padding: 5px 20px;
}

QTabBar::tab:selected {
  background-color: #cccccc;
  border: 1px solid transparent;
}

QTabBar::tab:hover {
  background-color: #298ce1;
  color: white;
}

QLineEdit,
QComboBox,
QSpinBox {
  background-color: white;
  border: 1px solid #cccccc;
  color: #111111;
  padding: 5px;
}

QLineEdit:disabled,
QComboBox:disabled,
QSpinBox:disabled {
  background-color: transparent;
  color: #bbbbbb;
}

QComboBox::drop-down {
  border: 0;
  margin-left: 10px;
  margin-right: 20px;
  margin-top: 5px;
}

QComboBox::down-arrow {
  border-bottom-color: #666666;
  border-bottom-style: none;
  border-left-width: 5px;
  border-right-width: 5px;
  border-style: solid;
  border-top-color: #666666;
  border-top-width: 5px;
  margin: 1px;
}

QSpinBox::up-button {
  border-bottom-color: #666666;
  border-bottom-width: 5px;
  border-left-width: 5px;
  border-right-width: 5px;
  border-style: solid;
  border-top-color: #666666;
  border-top-style: none;
  margin: 3px;
  width: 0;
}

QSpinBox::down-button {
  border-bottom-color: #666666;
  border-bottom-style: none;
  border-left-width: 5px;
  border-right-width: 5px;
  border-style: solid;
  border-top-color: #666666;
  border-top-width: 5px;
  margin: 3px;
  width: 0;
}

QComboBox::down-arrow:disabled {
  border-bottom-color: #cccccc;
  border-top-color: #cccccc;
}

QComboBox QAbstractItemView {
  background-color: white;
  border: 1px solid #cccccc;
  color: #111111;
}

QLabel,
QCheckBox,
QRadioButton {
  color: #111111;
}

QCheckBox::indicator {
  background-color: white;
  border: 1px solid #cccccc;
  height: 14px;
  width: 14px;
}

QCheckBox::indicator:checked {
  image: url(skin:controls/checkbox_check_lite.svg);
}

QRadioButton::indicator {
  background: white;
  border: 1px solid #cccccc;
  border-radius: 8px;
  height: 14px;
  width: 14px;
}

QRadioButton::indicator:checked {
  image: url(skin:controls/radio_check_lite.svg);
}

QCheckBox::indicator:pressed,
QRadioButton::indicator:pressed {
  background-color: #eeeeee;
}

QSlider::groove:horizontal {
  background: #eeeeee;
  border: 1px solid #cccccc;
  height: 6px;
}

QSlider::handle:horizontal {
  background: white;
  border: 1px solid #cccccc;
  border-radius: 2px;
  margin: -5px -1px;
  width: 20px;
}

QSlider::handle:horizontal:hover,
QSlider::handle:horizontal:pressed {
  background: #fefefe;
}

QSlider::handle:horizontal:disabled {
  background: transparent;
  border: 0;
  border-radius: 0;
  margin: 0;
}

#qteRichText {
  background-color: white;
  border: 1px solid #cccccc;
  padding: 5px;
}
